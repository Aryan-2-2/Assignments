package com.masai;

public class VowelOrConsonant {

	char c;
	
	void CheckVowelOrConsonant(char c) {
	if(c == 'a') {
		System.out.println("Vowel");
	}
	}
	
	
	
	public static void main(String[] args) {
		
		VowelOrConsonant charater = new VowelOrConsonant();
		
		charater.CheckVowelOrConsonant('a');
		
	}
}
