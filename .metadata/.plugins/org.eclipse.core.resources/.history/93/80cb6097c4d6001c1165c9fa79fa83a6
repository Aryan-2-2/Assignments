package com.masai;

public class Student {

     int roll;
     String name;
     int marks;
     
     void displayStudentDetails(int roll ) {
    	System.out.println(roll); 
    	 
     }
	
	public static void main(String[] args) {
		
		Student s1 = new Student();
		s1.roll = 12;
		s1.name = "Suresh";
		s1.marks = 9;
		
		s1.displayStudentDetails(12);
	}
}
