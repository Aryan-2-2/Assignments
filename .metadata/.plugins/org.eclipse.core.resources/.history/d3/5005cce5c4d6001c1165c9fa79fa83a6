package com.masai;

public class Student {

     int roll;
     String name;
     int marks;
     
     void displayStudentDetails(int roll ) {
    	System.out.println("Roll No :"+ roll); 
    	 
     }
	
	public static void main(String[] args) {
		
		Student s1 = new Student();
		
		
		s1.displayStudentDetails(12);
	}
}
